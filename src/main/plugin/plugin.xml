<SageTVPlugin>
       <Name>Live PVR Data Server</Name>
       <Identifier>livepvrdataserver</Identifier>
    <Description><![CDATA[
Provides Sports Event monitoring for use by the SRE plugin
    ]]></Description>
       <Author>skiingwiz</Author>
       <CreationDate>2016.04.23</CreationDate>
       <ModificationDate>@last-modified@</ModificationDate>
       <Version beta="true">@pluginVersion@</Version>
       <ResourcePath>plugins/livepvrdata-server</ResourcePath>
       <Webpage>https://github.com/livepvrdata-oss/livepvrdata-svc</Webpage>
       <Dependency>
               <JVM/>
               <MinVersion>1.7</MinVersion>
           </Dependency>
       <Dependency>
                <Core/>
                <MinVersion>7.1.9.1</MinVersion>
       </Dependency>
        <Dependency>
                <Plugin>groovy</Plugin>
                <MinVersion>2.0.5</MinVersion>
        </Dependency>
        <Dependency>
                <Plugin>jetty</Plugin>
                <MinVersion>2.3.0.14</MinVersion>
        </Dependency>
       <PluginType>Standard</PluginType>
       <Package>
               <PackageType>System</PackageType>
               <Location>@download-url-location@/livepvrdata-svc-war-@version@.zip</Location>
               <MD5>@war-file-checksum@</MD5>
               <Overwrite>true</Overwrite>
       </Package>
       <Package>
               <PackageType>System</PackageType>
               <Location>@download-url-location@/livepvrdata-svc-context-@version@.zip</Location>
               <MD5>@context-file-checksum@</MD5>
               <Overwrite>true</Overwrite>
       </Package>
           <ReleaseNotes><![CDATA[
* First release for sage server
           ]]></ReleaseNotes>
</SageTVPlugin>
